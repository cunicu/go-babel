    "version": "0.2.0",
    "configurations": [
    {
        "name": "Launch Ginkgo tests",
        "type": "go",
        "request": "launch",
        "mode": "test",
        "program": "${workspaceFolder}/${relativeFileDirname}",
        "asRoot": true,
        "console": "integratedTerminal",
        "args": [
            "-ginkgo.vv",
            // "-ginkgo.focus", "^SomeTest$"
            "--",
        ]
    }
    ]
}